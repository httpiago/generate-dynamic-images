{
  "version": 2,
  "builds": [
    { "src": "public/*", "use": "@now/static" },
    { "src": "package.json", "use": "@now/static-build" },
    { "src": "src/index.ts", "use": "@now/node@canary", "config": { "maxLambdaSize": "40mb" } }
  ],
  "routes": [
    { "src": "/", "dest": "/src/index.ts" }
  ]
}